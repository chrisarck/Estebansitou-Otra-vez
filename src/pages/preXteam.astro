---
const numbers = [];

for(let i = 1; i <= 10; i++){
    numbers.push(i);
}
---
<div class="row">
    <ul>
        {numbers.map(i => (
            <li>Equipo {i}</li>
        ))}
    </ul>
</div>
<style>
    li{
        
    }
    .row{
        background-color: rgba(136, 86, 29, 0);
        margin: 0;
    }
    ul{
        display: flex;
        justify-content: space-evenly;
        flex-direction: row;
        margin: 30px 20px;
        list-style: none;
        padding: 0;
        font-size: 2em;
    }
</style>